{
    "containers": {
        "stockApi": {
            "image": "",
            "ports": {
                "3000": "HTTP"
            }
        }
    },
    "publicEndpoint": {
        "containerName": "stockApi",
        "containerPort": 3000,
        "healthCheck": {
            "healthyThreshold": 2,
            "unhealthyThreshold": 2,
            "timeoutSeconds": 30,
            "intervalSeconds": 10,
            "path": "/"
        }
    }
}